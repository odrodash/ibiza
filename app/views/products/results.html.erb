<h2 class="my-4 text-center">Busqueda de Alquiler de Productos</h2>


<div class="cards">
  <% @products.each do |product| %>
    <%= link_to product_path(product) do %>
      <%= render 'shared/cards', product: product  %>
    <% end %>
  <% end %>
</div>

